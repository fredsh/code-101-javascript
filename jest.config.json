{
  "testRegex": "((\\.|/*.)(spec|test))\\.js?$",
  "transform": {},
  "testResultsProcessor": "jest-junit",
  "collectCoverage": true,
  "coverageReporters": [
    "text",
    "text-summary",
    "lcov"
  ],
  "collectCoverageFrom": [
    "**/*.js",
    "!**/*.test.js",
    "!**/node_modules/**",
    "!**/coverage/**"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
